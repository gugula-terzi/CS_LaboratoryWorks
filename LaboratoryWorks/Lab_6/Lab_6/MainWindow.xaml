    <Grid Margin="15">
        <Border Background="#FF303030" CornerRadius="5">
            <Border.Effect>
                <DropShadowEffect BlurRadius="20" Opacity="0.6" Direction="-85" Color="black" ShadowDepth="3"/>
            </Border.Effect>
        </Border>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="50"/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <Grid Grid.Row="0"
                  x:Name="NavPanel"
                  MouseDown="NavPanel_MouseDown">
                <Border Background="Transparent"/>
                <StackPanel Orientation="Horizontal"
                            HorizontalAlignment="Right"
                            VerticalAlignment="Center"
                            Margin="10">
                    <Button Width="30"
                            Height="30"
                            x:Name="MinimizeBtn"
                            Click="MinimizeBtn_Click"
                            Style="{DynamicResource NavPanelBtn}">
                        <materialDesign:PackIcon Kind="WindowMinimize"
                                                 Width="20"
                                                 Height="20"/>

                    </Button>
                    <Button Width="30"
                            Height="30"
                            x:Name="CloseBtn"
                            Click="CloseBtn_Click"
                            Style="{DynamicResource NavPanelBtn}">
                        <materialDesign:PackIcon Kind="CloseBold"
                                                 Width="20"
                                                 Height="20"/>
                    </Button>
                </StackPanel>
            </Grid>
            <Grid Grid.Row="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="200"/>
                    <ColumnDefinition/>
                </Grid.ColumnDefinitions>
                <Grid Grid.Column="0">
                    <StackPanel HorizontalAlignment="Center"
                                VerticalAlignment="Center">
                        <Button x:Name="DrawSunBtn"
                                Margin="0, 0, 0, 50"
                                Click="DrawSunBtn_Click"
                                Style="{DynamicResource MenuButtons}"
                                Height="50"
                                Width="160" 
                                FontFamily="Ink Free" 
                                FontWeight="Bold">

                            <StackPanel Orientation="Horizontal">
                                <Label Content="Draw"
                                       Margin="0, 0, 5, 0"
                                       VerticalAlignment="Center"
                                       VerticalContentAlignment="Center"
                                       Foreground="white"
                                       FontSize="24"
                                       Height="40"/>
                                <materialDesign:PackIcon Kind="WbSunny"
                                                         Width="30"
                                                         Height="30"
                                                         VerticalAlignment="Center"/>
                            </StackPanel>
                        </Button>
                        <Button x:Name="DrawRainBtn"
