                                Height="50"
                                Width="160" 
                                FontFamily="Ink Free" 
                                FontWeight="Bold">

                            <StackPanel Orientation="Horizontal">
                                <Label Content="Draw"
                                       Margin="0, 0, 5, 0"
                                       VerticalAlignment="Center"
                                       VerticalContentAlignment="Center"
                                       Foreground="white"
                                       FontSize="24"
                                       Height="40"/>
                                <materialDesign:PackIcon Kind="Cloud"
                                                         VerticalAlignment="Center"
                                                         Width="30"
                                                         Height="30"/>
                            </StackPanel>
                        </Button>
                        <Button x:Name="EnableFieldBtn"
